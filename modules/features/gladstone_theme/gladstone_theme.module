<?php
/**
 * @file
 * Code for the Gladstone Theme feature.
 */

include_once 'gladstone_theme.features.inc';

/**
 * Implementation of hook_ctools_plugin_directory()
 */
function gladstone_theme_ctools_plugin_directory($module, $plugin) {
  if ($module == 'page_manager' || $module == 'panels') {
    return 'plugins/' . $plugin;
  }
}